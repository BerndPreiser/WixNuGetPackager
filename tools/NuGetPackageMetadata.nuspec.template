<?xml version="1.0"?>
<package>
  <!-- WixNugetPackager will use the <metadata> element from this file to define the package metadata. 
       Any elements outside the <metadata> element will be ignored. The use of elements that affect
       the content of the package is not supported (eg, <references>).
       
       Replacement tokens are available as follows. The term "property" refers to MSBuild properties.
          
        $id$                The Wix project's OutputName property, or the WixNugetPackageId property
                            if defined.
                            
        $version$           The WixNugetPackageVersion property if defined, otherwise this replacement
                            token will not be available.
                            
        $author$            The WixNugetPackageAuthor property if defined, otherwise this replacement
                            token will not be available.
                            
        $description$       The WixNugetPackageDescription property if defined, otherwise this replacement
                            token will not be available.

        Note: the standard replacement token $references$ is not available.
  -->
  <metadata>
    <id>$id$</id>
    <version>1.0.0</version>
    <title>$id$</title>
    <authors>Package Authors</authors>
    <owners>Package Owners</owners>
    <licenseUrl>http://LICENSE_URL_HERE_OR_DELETE_THIS_LINE</licenseUrl>
    <projectUrl>http://PROJECT_URL_HERE_OR_DELETE_THIS_LINE</projectUrl>
    <iconUrl>http://ICON_URL_HERE_OR_DELETE_THIS_LINE</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Package description</description>
    <releaseNotes>Summary of changes made in this release of the package.</releaseNotes>
    <copyright>Copyright 2016</copyright>
    <tags>Tag1 Tag2</tags>
  </metadata>
</package>